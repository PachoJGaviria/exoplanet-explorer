{
  "name": "exoplanet-explorer",
  "version": "1.0.0",
  "description": "Tutorial about javascript promise",
  "main": "main.js",
  "engines": {
    "node": ">=10.15.3"
  },
  "scripts": {
    "start": "npm run standard && npm run standard-html && node main.js",
    "debug": "node --inspect-brk=9229 main.js",
    "test": "jest",
    "standard": "standard --verbose",
    "standard-html": "standard --verbose --plugin html '**/*.html'",
    "fix": "standard --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PachoJGaviria/exoplanet-explorer.git"
  },
  "keywords": [
    "Javascript",
    "promise",
    "planet"
  ],
  "author": "Francisco Gaviria <fxiajavier@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PachoJGaviria/exoplanet-explorer/issues"
  },
  "homepage": "https://github.com/PachoJGaviria/exoplanet-explorer#readme",
  "devDependencies": {
    "eslint-plugin-html": "^5.0.3",
    "jest": "^24.7.1",
    "standard": "^12.0.1"
  },
  "standard": {
    "globals": [
      "alert"
    ],
    "ignore": [
      "**/dist/"
    ]
  }
}
